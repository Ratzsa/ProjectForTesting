CFLAGS=-Wall -Werror -g

programmet.exe: main.o calculator.o checkinputs.o gamecode.o gamemenu.o shapes.o systemfunctions.o 
	gcc $(CFLAGS) -o programmet.exe main.o calculator.o checkinputs.o gamecode.o gamemenu.o shapes.o systemfunctions.o 

shapes.o: shapes.c
	gcc $(CFLAGS) -c shapes.c

calculator.o: calculator.c
	gcc $(CFLAGS) -c calculator.c

checkinputs.o: checkinputs.c
	gcc $(CFLAGS) -c checkinputs.c

gamecode.o: gamecode.c
	gcc $(CFLAGS) -c gamecode.c

gamemenu.o: gamemenu.c
	gcc $(CFLAGS) -c gamemenu.c

systemfunctions.o: systemfunctions.c
	gcc $(CFLAGS) -c systemfunctions.c

main.o: main.c
	gcc $(CFLAGS) -c main.c

clean:
	@del /q programmet.exe
	@del /q *.o

.PHONY: clean